{
  "presets": [
    [
      "env",
      {
        "exclude": [
          "transform-regenerator"
        ]
      }
    ],
    "react"
  ],
  "env": {
    "production": {
      "only": [
        "src",
        "kit"
      ],
      "plugins": [
        "transform-react-remove-prop-types",
        "transform-react-inline-elements",
        "transform-react-constant-elements"
      ]
    }
  },
  "plugins": [
    "add-module-exports",
    "syntax-dynamic-import",
    "transform-object-rest-spread",
    "transform-class-properties",
    "transform-regenerator"
  ]
}
